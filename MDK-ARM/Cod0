                            start = message.indexOf("ct=", fromIndex) + 3;
                            end = start + 3;    // %3i
                            fromIndex = end + 1;
                            buf = new char[end - start];
                            message.getChars(start, end, buf, 0);
                            val = new String(buf);
                            tvCountVal.setText(val);
                            //-- fl -- %3i --
                            start = message.indexOf("fl=", fromIndex) + 3;
                            end = start + 3;    // %3i
                            fromIndex = end + 1;
                            buf = new char[end - start];
                            message.getChars(start, end, buf, 0);
                            val = new String(buf);
                            tvFlapVal.setText(val+"%");
                            //-- fu -- %3i --
                            start = message.indexOf("fu=", fromIndex) + 3;
                            end = start + 3;    // %3i
                            fromIndex = end + 1;
                            buf = new char[end - start];

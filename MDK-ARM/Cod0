//------------------- ANDROID -------------------------------
                            //-- id -- %2i --
                            int fromIndex = 0;
                            String val;
                            int start = message.indexOf("id=", fromIndex) + 3;
                            int end = start + 2;    // %2i
                            fromIndex = end + 1;
                            char[] buf = new char[end - start];
                            message.getChars(start, end, buf, 0);
                            val = new String(buf);
                            tvCell.setText(getString(R.string.CellNum, val));
                            //-- t0 -- %.1f --
                            start = message.indexOf("t0=", fromIndex) + 3;
                            end = start + 4;    // %.1f
                            fromIndex = end + 1;
                            buf = new char[end - start];
                            message.getChars(start, end, buf, 0);
                            val = new String(buf);
                            tvDry0Val.setText(val);
                            //-- u0 -- %.1f --
                            start = message.indexOf("u0=", fromIndex) + 3;
                            end = start + 4;    // %.1f
                            fromIndex = end + 1;
                            buf = new char[end - start];
                            message.getChars(start, end, buf, 0);
                            val = new String(buf);
                            tvDry0U.setText("["+val+"]");
                            //-- t1 -- %.1f --
                            start = message.indexOf("t1=", fromIndex) + 3;
                            end = start + 4;    // %.1f
                            fromIndex = end + 1;
                            buf = new char[end - start];
                            message.getChars(start, end, buf, 0);
                            val = new String(buf);
                            tvHumVal.setText(val);
                            //-- u1 -- %.1f --
                            start = message.indexOf("u1=", fromIndex) + 3;
                            end = start + 4;    // %.1f
                            fromIndex = end + 1;
                            buf = new char[end - start];
                            message.getChars(start, end, buf, 0);
                            val = new String(buf);
                            tvHumU.setText("["+val+"]");
                            //-- t2 -- %.1f --
                            start = message.indexOf("t2=", fromIndex) + 3;
                            end = start + 4;    // %.1f
                            fromIndex = end + 1;
                            buf = new char[end - start];
                            message.getChars(start, end, buf, 0);
                            val = new String(buf);
                            int g;
                            if (!val.equals("")){
                                char myChar = val.charAt(0);
                                g = Integer.parseInt(String.valueOf(myChar));
                            }
                            else g = 10;
                            if (g>8) tvDry1Val.setVisibility(View.INVISIBLE);
                            else tvDry1Val.setText(val);
                            //-- t3 -- %.1f --
                            start = message.indexOf("t3=", fromIndex) + 3;
                            end = start + 4;    // %.1f
                            fromIndex = end + 1;
                            buf = new char[end - start];
                            message.getChars(start, end, buf, 0);
                            val = new String(buf);
                            if (!val.equals("")){
                                char myChar = val.charAt(0);
                                g = Integer.parseInt(String.valueOf(myChar));
                            }
                            else g = 10;
                            if (g>8) tvDry2Val.setVisibility(View.INVISIBLE);
                            else tvDry1Val.setText(val);
                            //-- tm -- %3i --
                            start = message.indexOf("tm=", fromIndex) + 3;
                            end = start + 3;    // %3i
                            fromIndex = end + 1;
                            buf = new char[end - start];
                            message.getChars(start, end, buf, 0);
                            val = new String(buf);
                            tvTimerVal.setText(val);
                            //-- tu -- %3i --
                            start = message.indexOf("tu=", fromIndex) + 3;
                            end = start + 3;    // %3i
                            fromIndex = end + 1;
                            buf = new char[end - start];
                            message.getChars(start, end, buf, 0);
                            val = new String(buf);
                            tvTimerU.setText("["+val+"]");
